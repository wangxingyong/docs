#!/bin/bash

PWD="`pwd`"
if [[ $PWD =~ ${MY_WORK_HOME}/work([0-9]+) ]] ; then
	USER_HOME="${BASH_REMATCH[0]}"
elif [[ $PWD =~ ${MY_WORK_HOME}/alibaba ]] ; then
	USER_HOME="${MY_WORK_HOME}/alibaba"
	USER_HOME="/home/leo"
else
	USER_HOME="/home/leo"
fi

##USER_HOME="/home/leo"
echo "proxy user home is: $USER_HOME"

